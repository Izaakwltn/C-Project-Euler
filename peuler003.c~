//Project Euler # 3
//Largest Prime Factor of 600851475143
#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int primep (n){
  double sr = sqrt(n);
  for (i = 2; i <= sr; i++){
    if (n % i == 0){
      return 0;
    }
  }
  return 1;
}

int main(){
  print("%d\n", primep(20));
  return 0;
}
